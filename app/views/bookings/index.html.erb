<table>
  <thead>
    <tr>
      <th>ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booking.each do |booking| %>
      <tr>
        <td><%= booking.id %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' },:remote => true,:class=>'delete_booking' %></td>
      </tr>
    <% end %>
  </tbody>
</table>